{
  "name": "template-fullstack-node-react",
  "version": "1.0.0",
  "description": "Full-stack project template with React frontend and Node.js/Express backend",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm --prefix backend run dev",
    "dev:frontend": "npm --prefix frontend start",
    "start": "npm --prefix backend start",
    "start:backend": "npm --prefix backend start",
    "start:frontend": "npm --prefix frontend start",
    "build": "npm --prefix frontend run build",
    "install:all": "npm install && npm --prefix backend install && npm --prefix frontend install && npm --prefix e2e install",
    "test": "npm --prefix backend test && npm --prefix frontend test -- --watchAll=false",
    "test:backend": "npm --prefix backend test",
    "test:frontend": "npm --prefix frontend test -- --watchAll=false",
    "test:e2e": "npm --prefix e2e test",
    "test:e2e:headed": "npm --prefix e2e run test:headed",
    "test:e2e:ui": "npm --prefix e2e run test:ui",
    "test:e2e:smoke": "npm --prefix e2e run test:smoke",
    "test:e2e:report": "npm --prefix e2e run report",
    "e2e:install": "npm --prefix e2e install && npm --prefix e2e run install-browsers",
    "e2e:codegen": "npm --prefix e2e run codegen",
    "lint": "npm --prefix backend run lint && npm --prefix frontend run lint",
    "lint:fix": "npm --prefix backend run lint:fix && npm --prefix frontend run lint:fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf node_modules backend/node_modules frontend/node_modules e2e/node_modules",
    "prepare": "echo 'Ready to run npm run install:all'"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "node",
    "express",
    "mongodb",
    "fullstack",
    "template",
    "jwt",
    "redux",
    "playwright",
    "e2e"
  ],
  "author": "",
  "license": "MIT"
}
